<mbean>
      <description><![CDATA[<b>Content Edit Service for dcm4chee-web3</b>
		]]> </description>

    <descriptors>
       <persistence persistPolicy="OnUpdate"/>
       <persistence-manager value="org.jboss.mx.persistence.DelegatingPersistenceManager" />
    </descriptors>

    <class>org.dcm4chee.web.service.contentedit.ContentEditService</class>

    <constructor>
         <description>The default constructor</description>
         <name>ContentEditService</name>
    </constructor>

   <attribute access="read-write"
      getMethod="getUIDRoot"
      setMethod="setUIDRoot">
      <description>Root part of new generated UIDs.
      </description>
      <name>UIDRoot</name>
      <type>java.lang.String</type>
      <descriptors>
         <value value='1.2.40.0.13.1.1' />
      </descriptors>
   </attribute>

   <attribute access="read-write"
      getMethod="getRejectionNoteCode"
      setMethod="setRejectionNoteCode">
      <description>Concept Name Code used for Rejection Note Key Object Selection.
      </description>
      <name>RejectionNoteCode</name>
      <type>java.lang.String</type>
      <descriptors>
         <value value='(113001, DCM, "Rejected for Quality Reasons")' />
      </descriptors>
   </attribute>

   <attribute access="read-write"
      getMethod="isProcessRejNote"
      setMethod="setProcessRejNote">
      <description>Enable/Disable sending Rejection Note.
      </description>
      <name>enableRejNote</name>
      <type>boolean</type>
      <descriptors>
         <value value="true" />
      </descriptors>
   </attribute>
   <attribute access="read-write"
      getMethod="isUseIOCM"
      setMethod="setUseIOCM">
      <description><![CDATA[Enable IOCM for propagation of changes for Study, Series and Instance level.<br />
      	If enabled, changes are sent via RejectionNote and changed Instances instead of private Attribute Modification SOP class.<br />
		      	
      ]]></description>
      <name>UseIOCM</name>
      <type>boolean</type>
      <descriptors>
         <value value="false" />
      </descriptors>
   </attribute>
   <attribute access="read-write"
      getMethod="getIocmForwardDelay"
      setMethod="setIocmForwardDelay">
      <description><![CDATA[Delay in milliseconds to send modified objects to IOCM archive.<br />
      		This delay may assure that the rejection note is processed before the modified objects are sent.<br />
      		(e.g. for move study to patient)
      ]]></description>
      <name>IocmForwardDelay</name>
      <type>long</type>
      <descriptors>
         <value value="0" />
      </descriptors>
   </attribute>

   <attribute access="read-write"
      getMethod="getContributingEquipment"
      setMethod="setContributingEquipment">
      <description><![CDATA[ContributingEquipment used in Referenced Instance Sequence for IOCM. 
      	Format:&lt;InstitutionName&gt;|&lt;StationName&gt;|&lt;Manufacturer&gt;
      ]]></description>
      <name>ContributingEquipment</name>
      <type>java.lang.String</type>
      <descriptors>
         <value value='InstitutionName|StationName|Manufacturer' />
      </descriptors>
   </attribute>
   <attribute access="read-write"
      getMethod="isProcessIAN"
      setMethod="setProcessIAN">
      <description>Enable/Disable sending Instance Availability Notification.
      </description>
      <name>enableIAN</name>
      <type>boolean</type>
      <descriptors>
         <value value="true" />
      </descriptors>
   </attribute>
   <attribute access="read-write"
      getMethod="isSendIANonMppsLinked"
      setMethod="setSendIANonMppsLinked">
      <description>Enable/Disable sending Instance Availability Notification after linking MPPS with MWL.
      </description>
      <name>sendIANonMppsLinked</name>
      <type>boolean</type>
      <descriptors>
         <value value="false" />
      </descriptors>
   </attribute>
   <attribute access="read-write"
      getMethod="isNotifyArchiveServicesAfterLinking"
      setMethod="setNotifyArchiveServicesAfterLinking">
      <description><![CDATA[Enable/Disable JMX notification of Archive services after linking MPPS with MWL.<br />
      		Use <code>ArchiveIANScuServicename.notifyOtherServices</code> to send the notification.<br />
      		Standard services which get notified: XDS-I, ExportManager, GPWLFeed and UPSFeed.
      ]]></description>
      <name>NotifyArchiveServicesAfterLinking</name>
      <type>boolean</type>
      <descriptors>
         <value value="false" />
      </descriptors>
   </attribute>
   <attribute access="read-write"
      getMethod="getForwardModifiedToAETs"
      setMethod="setForwardModifiedToAETs">
      <description>List of AETs to forward modified objects to system that doesn't support Attribute Modification Notification.
      		Use NONE to disable this feature. 
      </description>
      <name>ForwardModifiedToAETs</name>
      <type>java.lang.String</type>
      <descriptors>
         <value value="NONE" />
      </descriptors>
   </attribute>
   <attribute access="read-write"
      getMethod="isEnableForwardOnPatientUpdate"
      setMethod="setEnableForwardOnPatientUpdate">
      <description><![CDATA[Enable forwarding of instances if patient attributes are modified.<br />
        If enabled, one instance (online) is sent to all AE's listed in <b>ForwardModifiedToAETs</b><br />
      	You should enable this feature only if you have to update a system that does NOT support HL7 ADT update messages!
      ]]></description>
      <name>EnableForwardOnPatientUpdate</name>
      <type>boolean</type>
      <descriptors>
         <value value="false" />
      </descriptors>
   </attribute>

   <attribute access="read-write"
      getMethod="getModifyingSystem"
      setMethod="setModifyingSystem">
      <description>Identification of the system which modifies attributes. Used in OriginalAttributesSequence.
      </description>
      <name>ModifyingSystem</name>
      <type>java.lang.String</type>
      <descriptors>
         <value value="DCM4CHEE" />
      </descriptors>
   </attribute>

   <attribute access="read-write"
      getMethod="getModifyReason"
      setMethod="setModifyReason">
      <description>Default Reason for attribute modifications. Used in OriginalAttributesSequence.
      </description>
      <name>ModifyReason</name>
      <type>java.lang.String</type>
      <descriptors>
         <value value="COERCE" />
      </descriptors>
   </attribute>

   <attribute access="read-write"
      getMethod="getCoerceConfigDir"
      setMethod="setCoerceConfigDir">
      <description><![CDATA[Directory where configuration files for attribute
      coercion are located: if XSL stylesheet "mwl-cfindrsp2cstorerq.xsl" exists in this directory 
      or in a sub-directory equal to the ScheduledStationAETitle, Serien and Study attributes will be updated 
      with Request attributes from MWL item according these stylesheets.
      A relative path name is resolved relative to
      <i>archive-install-directory</i>/server/default/.]]>  
      </description>
      <name>CoerceConfigDirectory</name>
      <type>java.lang.String</type>
      <descriptors>
         <value value="conf/dcm4chee-ae"/>
      </descriptors>
   </attribute>
   <attribute access="read-write"
      getMethod="isDcm14Stylesheet"
      setMethod="setDcm14Stylesheet">
      <description>Set true if the mwl-cfindrsp2cstorerq.xsl stylesheet use the dcm4che14 xml format.
      </description>
      <name>Dcm14Stylesheet</name>
      <type>boolean</type>
      <descriptors>
         <value value="true" />
      </descriptors>
   </attribute>
   <attribute access="read-write"
      getMethod="getDcm2To14Tpl"
      setMethod="setDcm2To14Tpl">
      <description>Stylesheet to transform dcm4che2 formatted xml to dcm4che14 xml.
      </description>
      <name>Dcm2To14XSL</name>
      <type>java.lang.String</type>
      <descriptors>
         <value value="resource:xsl/dcm2To14.xsl" />
      </descriptors>
   </attribute>
   <attribute access="read-write"
      getMethod="getDcm14To2Tpl"
      setMethod="setDcm14To2Tpl">
      <description>Stylesheet to transform dcm4che14 formatted xml to dcm4che2 xml.
      </description>
      <name>Dcm14To2XSL</name>
      <type>java.lang.String</type>
      <descriptors>
         <value value="resource:xsl/dcm14To2.xsl" />
      </descriptors>
   </attribute>
   
   <attribute access="read-write"
      getMethod="getAddMwlAttrsToMppsXsl"
      setMethod="setAddMwlAttrsToMppsXsl">
      <description><![CDATA[ Stylesheet to add MWL attributes to MPPS attributes for 'MPPS Linked' notification.<br/>
                This allow the use of additional MWL attributes to create ORM message in HL7SendV2 service. 
                A relative path name is resolved relative to <i>archive-install-directory</i>/server/default/.<br/>
                NONE..leave MPPS attributes unchanged. <br />
                Use a leading '14|' identifier if the stylesheet is for dcm4che14 formatted xml.
        ]]>
      </description>
      <name>AddMwlAttrsToMppsXsl</name>
      <type>java.lang.String</type>
      <descriptors>
         <value value="NONE"/>
      </descriptors>
   </attribute>

   <attribute access="read-write"
      getMethod="isUpdateMwlStatus"
      setMethod="setUpdateMwlStatus">
      <description><![CDATA[ Enable update of MWL status after linking/unlinking MPPS to MWL. <br />
                If enabled the MWL status is set to <i>COMPLETED</i> after linking and <i>SCHEDULED</i> after unlink! 
        ]]>
      </description>
      <name>UpdateMwlStatus</name>
      <type>boolean</type>
      <descriptors>
         <value value="true"/>
      </descriptors>
   </attribute>

   <attribute access="read-write"
      getMethod="getRejectionNoteServiceName"
      setMethod="setRejectionNoteServiceName">
      <description>Used internally. Do NOT modify.
      </description>
      <name>RejectionNoteServiceName</name>
