<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
    "http://www.w3.org/2002/08/xhtml/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:wicket="http://wicket.apache.org/dtds.data/wicket-xhtml1.4-strict.dtd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.w3.org/1999/xhtml
                          http://www.w3.org/2002/08/xhtml/xhtml1-strict.xsd">
	<body class="msgWindow">
		<div style="height: 85%; width: 95%; position: absolute; left: 15px; top: 40px;">
		    <span wicket:id="info">Instance INFO</span>
	        <wicket:container wicket:id="fragment" />
	        <wicket:fragment wicket:id="url">
	        	<div wicket:id="mimeCtr">
	        		<span wicket:id="contentTypeLabel">Content Type:</span><select wicket:id="mimeTypes" />
	        	</div>
	        	<div style="width: 100%; height:100%; background-color: #FFFFFF;">
		        	<iframe wicket:id="iframe" width="100%" height="600px">
						IFrame is not supported!
					</iframe>
				</div>
	        </wicket:fragment>
	      <wicket:fragment wicket:id="image">
	       	<div wicket:id="imgCtr">
	       	 	<span>Frame:</span>
				<a wicket:id="prev" style="text-decoration: none;">
					<img wicket:id="prevImg" />
				</a>
				<span wicket:id="frame" /><span>/</span><span wicket:id="nrOfFrames" />
				<a wicket:id="next" style="text-decoration: none;">
					<img wicket:id="nextImg" />
				</a>
				<span wicket:id="origLabel">orig</span>
				<input wicket:id="orig" type="checkbox" />
				<span wicket:id="pngLabel">PNG</span>
				<input wicket:id="png" type="checkbox" />
	        </div>
			<img wicket:id="wadoImg" />
	      </wicket:fragment>
        </div>
	</body>
</html>