<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:wicket="http://wicket.apache.org">
<head>
</head>
<body>
    <wicket:panel>
        <div id="tc-view-bibliography-header" class="tc-view-bibliography-header">
			<div id="tc-view-bibliography-title"
				class="tc-view-bibliography-title">
				<span wicket:id="tc-view-bibliography-title-text" />
			</div>
			<div id="tc-view-bibliography-options"
				class="tc-view-bibliography-options">
				<button id="tc-view-bibliography-add-btn"
					wicket:id="tc-view-bibliography-add-btn">
					<img wicket:id="tc-view-bibliography-add-img" /> <span
						wicket:id="tc-view-bibliography-add-text" />
				</button>
			</div>
        </div>
        <hr id="tc-view-bibliography-hr"/>
        <div id="tc-view-bibliography-container" wicket:id="tc-view-bibliography-container">
			<div wicket:id="tc-view-bibliography-list" class="tc-view-bibliography-list">
			     <a wicket:id="tc-view-bibliography-remove-btn" class="tc-view-bibliography-remove-btn">
                     <img wicket:id="tc-view-bibliography-remove-img" />
                 </a>
			     <textarea wicket:id="tc-view-bibliography-text" class="tc-view-bibliography-text ui-textarea">
				 </textarea>
			</div>
		</div>
	</wicket:panel> 
</body>
</html>