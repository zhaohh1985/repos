<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
    "http://www.w3.org/2002/08/xhtml/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:wicket="http://wicket.apache.org/dtds.data/wicket-xhtml1.4-strict.dtd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.w3.org/1999/xhtml
                          http://www.w3.org/2002/08/xhtml/xhtml1-strict.xsd">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	</head>
	<body>
	   <wicket:panel>
            <div class="msgWindow">
	    		<div class="edit-title">
					<wicket:message key="change_password.title" class="edit-title" />
				</div>
				<form wicket:id="change-password-form">				
					 <div class="edit-content">
					 	<div class="edit-table-panel">
		          			<div class="edit-table-row">
								<span wicket:id="old-password-label" class="edit-desc">
									<wicket:message key="change_password.old_password.label" />
								</span>
								<span wicket:id="for-user-label" class="edit-desc" />
								<input wicket:id="change_password.old_password.input" class="edit-value" type="password" size="15" />
							</div>
		          			<div class="edit-table-row">
		          				<div class="edit-desc">
									<wicket:message key="change_password.new_password_1.label" />
								</div>
								<input wicket:id="change_password.new_password_1.input" class="edit-value" type="password" size="15" />
							</div>					
		          			<div class="edit-table-row">
								<div class="edit-desc">
									<wicket:message key="change_password.new_password_2.label" />
								</div>
								<input wicket:id="change_password.new_password_2.input" class="edit-value" type="password" size="15" />
							</div>
						</div>
						<div class="edit-buttons">
							<input wicket:id="change-password-submit" type="submit" wicket:message="value:change_password.submit" />
						</div>
		                <div class="edit-title">
		                    <div wicket:id="resultLabel" class="edit-title" />
		                </div>
					</div>
				</form>
            </div>
        </wicket:panel>
	</body>
</html>