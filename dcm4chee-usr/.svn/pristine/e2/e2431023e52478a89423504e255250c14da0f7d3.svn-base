<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
    "http://www.w3.org/2002/08/xhtml/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:wicket="http://wicket.apache.org/dtds.data/wicket-xhtml1.4-strict.dtd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.w3.org/1999/xhtml
                          http://www.w3.org/2002/08/xhtml/xhtml1-strict.xsd">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	</head>
	<body>
	<wicket:panel>
		<div wicket:id="aet-group-window" />
        <div wicket:id="aet-assignment-window" />
		<div wicket:id="confirmation-window" />
		<div style="clear: both; margin-top: 5px;" />
		<button wicket:id="toggle-aet-group-form-link">
			<img wicket:id="toggle-aet-group-form-image" alt="New AET group" />
			<span wicket:id="aetgrouplist.add-aet-group-form.title" />
		</button>
		<table rules="groups" class="content-table" style="clear: both; margin-top: 9px;">
		  <thead>
		    <tr>
				<th style="width: 10%">
					<wicket:message key="aetgrouplist.table.groupname.title">
						[Groupname]
					</wicket:message>
				</th>
                <th></th>
                <th></th>
                <th></th>
                <th>
                    <wicket:message key="aetgrouplist.table.assignedAETs.title">
                        [Assigned AETs]
                    </wicket:message>
                </th>
                <th style="width: 100%"></th>
		    </tr>
		  </thead>
		  <tbody>
            <tr>
                <td wicket:id="groupname">
                    [Rolename]
                </td>
                <td>
                </td>
                <td>
                </td>
                <td>
                </td>
                <td>
                    <div wicket:id="assigned-aets" />
                </td>
                <td></td>
            </tr>
		    <tr wicket:id="aet-group-rows">
				<td wicket:id="groupname">
		      		[Rolename]
		      	</td>
                <td>
                    <a wicket:id="edit-aet-group-link" href="#">
                        <img wicket:id="aetgrouplist.edit.image" />
                    </a>
                </td>
                <td>
                    <a wicket:id="remove-aet-group-link" href="#">
                        <img wicket:id="aetgrouplist.delete.image" />
                    </a>
                </td>
                <td>
	                <a wicket:id="aetAssignment-link" href="#">
	                    <img wicket:id="aetgrouplist.aetAssignment.image" />
	                </a>
                </td>
                <td>
                    <div wicket:id="assigned-aets" />
                </td>
                <td></td>
		    </tr>
		  </tbody>
		</table>
		</wicket:panel>
	</body>
</html>	